class Solution(object):
    def kidsWithCandies(self, candies, extraCandies):
        """
        :type candies: List[int]
        :type extraCandies: int
        :rtype: List[bool]
        """
        result=[]
        i=0
        max_candies=max(candies)
        while i<len(candies):
            total=candies[i]+extraCandies
            if total>=max_candies:
                result.append(True)
            else:
                result.append(False)
            i+=1
        return result
                
        
